{
  "name": "Trello Card Dependency",
  "version": "1.0.4",
  "manifest_version": 2,
  "homepage_url": "https://github.com/amcmillan01/trello_card_dependency",
  "icons": {
    "48": "logo.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "bg/index.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://trello.com/*"
      ],
      "js": [
        "jquery-2.2.2.min.js",
        "cs/index.js"
      ],
      "css": [
        "cs/styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "cs/*",
    "logo-gray.png"
  ],
  "description": "Adds card dependency capability to Trello's web interface"
}